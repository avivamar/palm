# Ignore build outputs in packages
packages/*/.next
packages/*/dist
packages/*/.turbo
packages/*/coverage
packages/*/.cache
packages/*/node_modules

# Ignore admin package completely for Vercel deployment
packages/admin

# But don't ignore source files for other packages
!packages/*/src
!packages/*/package.json
!packages/*/tsconfig.json

# Re-ignore admin package files
packages/admin/package.json
packages/admin/tsconfig.json