{
  "name": "@rolitt/referral",
  "version": "1.0.0",
  "description": "Rolitt referral system MVP - simple and pluggable",
  "author": "Rolitt Team",
  "license": "MIT",
  "keywords": [
    "referral",
    "mvp",
    "rolitt",
    "nextjs",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./tracking": {
      "types": "./dist/tracking.d.ts",
      "default": "./dist/tracking.js"
    },
    "./admin": {
      "types": "./dist/admin/index.d.ts",
      "default": "./dist/admin/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.7.3"
  }
}
